
  
  {
    "store.product": {
      "children": ["responsive-layout.desktop#desktop__product-detail","responsive-layout.mobile#desktop__product-detail"]
    },
  
    "responsive-layout.mobile#desktop__product-detail": {
      "title": "Product Detail Desktop",
      "children": ["flex-layout.row#desktop__product-detail-mobile"]
    },
    "flex-layout.row#desktop__product-detail-mobile": {
      "title": "Product Detail Desktop Seccion",
      "children": ["flex-layout.col#desktop__product-detail"]
    },
    "flex-layout.col#desktop__product-detail": {
      "title": "Product Detail Seccions",
      "children": [
        "flex-layout.row#global__product--breadcrumb",
        "flex-layout.row#image_banner-pdp-mobile",
        "flex-layout.row#desktop__product--information",
        "flex-layout.row#product__tabs"
      ],
      "props": {
        "blockClass":"main-details-mobile"
      }
    },
    "flex-layout.row#image_banner-pdp-mobile":{
      "children": [
        "flex-layout.col#image_banner-mobile"
      ]
    },
    "flex-layout.col#image_banner-mobile":{
      "children": [
        "image#image_banner-pdp"
      ]
    },
  
    "image#image_banner-pdp":{
      "props": {
        "src":"assets/img/banner/banner-pdp.jpg"
      }
    }
  
  
  }
  
